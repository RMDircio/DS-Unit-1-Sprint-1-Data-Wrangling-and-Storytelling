      "source": [
        "# For Loop to change 'loan_status' to numerical\n",
        "\n",
        "status_list = []\n",
        "\n",
        "for cell in loans_df['loan_status']:\n",
        "    if cell == 'Current' or cell == 'Fully Paid':\n",
        "      status_list.append(1)\n",
        "    else: \n",
        "      status_list.append(0)\n"
      ],
      "execution_count": 0,
